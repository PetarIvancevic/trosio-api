{
  "name": "trosio-api",
  "version": "0.0.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "dotenv-safe": "^5.0.1",
    "google-auth-library": "^1.6.1",
    "is-promise": "^2.1.0",
    "joi": "^13.3.0",
    "jsonwebtoken": "^8.2.1",
    "kcors": "^2.2.1",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^4.0.0",
    "koa-jwt": "^3.3.2",
    "koa-mount": "^3.0.0",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "migratio": "^2.0.3",
    "moment": "^2.22.1",
    "nested-error-stacks": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "pg-promise": "^8.4.4",
    "slugify": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "ngrok": "^3.0.1",
    "nodemon": "^1.17.4",
    "sinon": "^6.1.5",
    "supertest": "^3.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  },
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "scripts": {
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "node -r babel-register -r ./env migrate.js",
    "dev": "run-p -s dev:server",
    "dev:expose": "node bin/ngrok.js",
    "dev:server": "nodemon -q -I -e js,json -i package.json -- -r babel-register server.js",
    "test": "tape -r babel-register -r ./env **/*.test.js | tap-spec",
    "db:recreate": "npm run migrate recreate",
    "db:seed": "npm run migrate seed",
    "ci": "run-s -s lint:check db:recreate db:seed test"
  }
}
